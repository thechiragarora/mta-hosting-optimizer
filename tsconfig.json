{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es2017",
      "noImplicitAny": false,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "esModuleInterop": false,
      "allowJs": false /* Allow javascript files to be compiled. */,
      "checkJs": false /* Report errors in .js files. */,
      "sourceMap": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "baseUrl": ".",
      "paths": {
        "*": [
          "node_modules/*",
          "src/types/*"
        ]
      },
      "types": ["jest"]
    },
    "exclude": ["dist"],
    "include": [
      "src",
    ],
    "types": ["src/types"]
  }
  